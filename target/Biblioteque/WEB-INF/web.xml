<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>DashboardServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.DashboardServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpruntListServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.EmpruntListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LivreListServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.LivreListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MembreListServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.MembreListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MembreDetailsServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.MembreDetailsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MembreDeleteServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.MembreDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MembreAddServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.MembreAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LivreDetailsServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.LivreDetailsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LivreAddServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.LivreAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LivreDeleteServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.LivreDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpruntAddServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.EmpruntAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpruntReturnServlet</servlet-name>
    <servlet-class>com.intellij.librarymanager.servlet.EmpruntReturnServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>DashboardServlet</servlet-name>
    <url-pattern>/dashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpruntListServlet</servlet-name>
    <url-pattern>/emprunt_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LivreListServlet</servlet-name>
    <url-pattern>/livre_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MembreListServlet</servlet-name>
    <url-pattern>/membre_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MembreDetailsServlet</servlet-name>
    <url-pattern>/membre_details</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MembreDeleteServlet</servlet-name>
    <url-pattern>/membre_delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MembreAddServlet</servlet-name>
    <url-pattern>/membre_add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LivreDetailsServlet</servlet-name>
    <url-pattern>/livre_details</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LivreAddServlet</servlet-name>
    <url-pattern>/livre_add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LivreDeleteServlet</servlet-name>
    <url-pattern>/livre_delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpruntAddServlet</servlet-name>
    <url-pattern>/emprunt_add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpruntReturnServlet</servlet-name>
    <url-pattern>/emprunt_return</url-pattern>
  </servlet-mapping>

</web-app>

